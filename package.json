{
  "name": "summoner",
  "version": "0.2.2",
  "description": "Manage 5e Statblocks",
  "main": "index.html",
  "scripts": {
    "browserify": "browserify src/app/main.jsx -o public/assets/js/bundle.js -t [babelify]",
    "build": "npm run mkdirs && npm run cp:static && npm run browserify && npm run compile:sass",
    "compile:sass": "node-sass --source-map true --output-style compressed src/css/style.scss public/assets/css/style.css",
    "cp:static": "cp src/static/index.html public/index.html",
    "lint": "eslint --plugin react --ext .js --ext .jsx src/app/",
    "mkdirs": "mkdir -p public/assets/css && mkdir -p public/assets/js",
    "test": "mocha --compilers js:babel-register",
    "watch:test": "mocha --compilers js:babel-register -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkrayer/summoner.git"
  },
  "author": "Jim Krayer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkrayer/summoner/issues"
  },
  "homepage": "https://github.com/jkrayer/summoner#readme",
  "devDependencies": {
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-register": "6.11.6",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "chai": "3.5.0",
    "eslint": "3.0.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-html": "1.4.0",
    "eslint-plugin-import": "1.10.3",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "5.0.1",
    "jsdom": "9.4.1",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "normalize-css": "2.3.1",
    "react-addons-pure-render-mixin": "15.2.1",
    "react-addons-test-utils": "15.2.1",
    "redux-devtools": "3.3.1"
  },
  "dependencies": {
    "marked": "0.3.5",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.5.1",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2"
  }
}
